{
  "name": "framework-compare",
  "version": "0.0.1",
  "private": true,
  "description": "Compare implementations of some basic webpages in different frameworks",
  "main": "index.js",
  "scripts": {
    "start": "node ./scripts/dev.js",
    "build": "node ./scripts/build.js",
    "dev": "node ./scripts/dev.js",
    "deploy": "npm run build && npm test && node ./scripts/deploy.js",
    "serve": "sirv --dev",
    "test": "jest",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,vue}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewiggins/framework-compare.git"
  },
  "author": "Andre Wiggins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewiggins/framework-compare/issues"
  },
  "homepage": "https://github.com/andrewiggins/framework-compare#readme",
  "workspaces": [
    "./frameworks/*",
    "./scripts/"
  ],
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,html,vue}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@rollup/plugin-node-resolve": "15.0.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/jest": "^29.0.0",
    "babel-jest": "^29.6.2",
    "gh-pages": "^3.1.0",
    "html-minifier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-puppeteer": "^9.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.2",
    "puppeteer": "^21.0.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.0",
    "rollup-plugin-visualizer": "^5.9.0"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "transform": {
      "^.+\\.[t|j]sx?$": "<rootDir>/tests/babelTransformer.js"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "trailingComma": "none"
  }
}
